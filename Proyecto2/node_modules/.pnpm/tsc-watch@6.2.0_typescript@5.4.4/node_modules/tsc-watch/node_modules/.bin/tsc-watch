#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/User/Documents/USAC/VII Semestre/Compi 1/Proyectos/OLC1_Proyecto2_202109750/Proyecto2/node_modules/.pnpm/tsc-watch@6.2.0_typescript@5.4.4/node_modules/tsc-watch/dist/lib/node_modules:/mnt/c/Users/User/Documents/USAC/VII Semestre/Compi 1/Proyectos/OLC1_Proyecto2_202109750/Proyecto2/node_modules/.pnpm/tsc-watch@6.2.0_typescript@5.4.4/node_modules/tsc-watch/dist/node_modules:/mnt/c/Users/User/Documents/USAC/VII Semestre/Compi 1/Proyectos/OLC1_Proyecto2_202109750/Proyecto2/node_modules/.pnpm/tsc-watch@6.2.0_typescript@5.4.4/node_modules/tsc-watch/node_modules:/mnt/c/Users/User/Documents/USAC/VII Semestre/Compi 1/Proyectos/OLC1_Proyecto2_202109750/Proyecto2/node_modules/.pnpm/tsc-watch@6.2.0_typescript@5.4.4/node_modules:/mnt/c/Users/User/Documents/USAC/VII Semestre/Compi 1/Proyectos/OLC1_Proyecto2_202109750/Proyecto2/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/User/Documents/USAC/VII Semestre/Compi 1/Proyectos/OLC1_Proyecto2_202109750/Proyecto2/node_modules/.pnpm/tsc-watch@6.2.0_typescript@5.4.4/node_modules/tsc-watch/dist/lib/node_modules:/mnt/c/Users/User/Documents/USAC/VII Semestre/Compi 1/Proyectos/OLC1_Proyecto2_202109750/Proyecto2/node_modules/.pnpm/tsc-watch@6.2.0_typescript@5.4.4/node_modules/tsc-watch/dist/node_modules:/mnt/c/Users/User/Documents/USAC/VII Semestre/Compi 1/Proyectos/OLC1_Proyecto2_202109750/Proyecto2/node_modules/.pnpm/tsc-watch@6.2.0_typescript@5.4.4/node_modules/tsc-watch/node_modules:/mnt/c/Users/User/Documents/USAC/VII Semestre/Compi 1/Proyectos/OLC1_Proyecto2_202109750/Proyecto2/node_modules/.pnpm/tsc-watch@6.2.0_typescript@5.4.4/node_modules:/mnt/c/Users/User/Documents/USAC/VII Semestre/Compi 1/Proyectos/OLC1_Proyecto2_202109750/Proyecto2/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/lib/tsc-watch.js" "$@"
else
  exec node  "$basedir/../../dist/lib/tsc-watch.js" "$@"
fi
